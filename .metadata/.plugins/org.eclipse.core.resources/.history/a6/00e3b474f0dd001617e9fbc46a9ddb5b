#include <iostream>
#include <string>
#include <cstdio>
#include <algorithm>
using namespace std;
struct Student{
	string id;
	int mark;
	int group;
}stu[30011];
bool cmp(Student a, Student b){
	return a>b;
}
int main() {
//	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	int count = 0;
	int n;
	cin>>n;
	for(int i = 0 ; i < n; i++){
		int m;
		cin>>m;
		for(int j = 0 ; j < m ; j++){
			cin>>stu[count].id>>stu[count].mark;
			stu[count].group = i;
			count++;
		}
	}
	int gouprank[101][300]={0};

	return 0;
}
