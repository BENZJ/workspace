#include <iostream>
#include <algorithm>
#include <cstdio>
using namespace std;
const int maxn = 10010;
int n,k,m;
struct Student{
	int id;
	bool flag;
	int score[6];
	int score_all;
	int solve;
}stu[maxn];
bool cmp(Student a ,Student b){
	if(a.flag!= b.flag)
		return a.flag>b.flag;
	else if(a.score_all != b.score_all){
		return a.score_all>b.score_all;
	}
	else if(a.solve != b.solve){
		return a.solve>b.solve;
	}
	else return a.id<b.id;
}
void init (){
	for(int i = 1 ; i <=n ; i++){
		stu[i].id = i;
		stu[i].score_all = 0;
		stu[i].solve = 0;
		stu[i].flag = false;
	}
}
int main() {
	init();
	scanf("%d%d%d",&n,&k,&m);
	return 0;
}
