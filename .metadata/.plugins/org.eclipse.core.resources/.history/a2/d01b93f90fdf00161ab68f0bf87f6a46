#include <iostream>
#include <algorithm>
#include <cstdio>
using namespace std;
struct Student{
	bool flag;
	int wish[5];
	int GE,GI;
	int Rank;
}stu[40010];
struct School{
	int num;
	int plan;
	int lastnumber;
}sch[110];
bool cmp(Student a , Student b){
	return (a.GE+a.GI)>(b.GE+b.GI);
}
int main() {
	int n,m,k;
	scanf("%d%d%d",&n,&m,&k);
	for(int i = 0 ; i < m ; i++){
		scanf("%d",&sch[i].plan);
	}
	return 0;
}
